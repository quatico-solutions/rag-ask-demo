{
  "watch": ["src", "data", ".env"],
  "ext": "ts,tsx,js,json,md",
  "ignore": [
    "node_modules/**/*",
    "dist/**/*",
    "build/**/*",
    "**/embeddings/**/*.json",
    "test-results/**/*",
    ".git/**/*",
    "**/*.test.ts",
    "**/*.test.js",
    "coverage/**/*"
  ],
  "exec": "tsx",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true
}
